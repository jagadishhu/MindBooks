pipeline {
	agent any
	stages {
		stage('Build-1st Stage') {
			steps {
				echo "Build stage- Running"
			}
		}
		stage('Testing-2nd Stage') {
			steps {
				input('Do you want to proceed ?')
			}
		}
		stage('Deploy-3rd Stage') {
			when {
				not {
					branch "master"
			}
		}
			steps {
				echo "Hello"
				}
		}
		stage('Monitoring Stage-4th Stage') {
		            steps {
		                        echo "Running the Unit Test"
		                    }
		            }
		}
	}
		          
